<% end %>
  <div class="tournament4-wrap" data-group="FF" id="group-FF">
    <h2>Final Four</h2>
    <div class="round3-top winner3" data-rank="sxexwxm">&nbsp;</div>
    <div class="round2-topwrap">
      <div class="round2-top" data-rank="sxe">&nbsp;</div>
      <div class="round1-top" id="South" data-rank="s">&nbsp;</div>
      <div class="round1-bottom" id="East" data-rank="e">&nbsp;</div>
