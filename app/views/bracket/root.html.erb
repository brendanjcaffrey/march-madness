<div class="container">
  <div class="page-header" id="bracket">
    <h1>March Madness Bracket</h1>
  </div>

<% @groups.each do |group, teams| %>
  <div class="tournament16-wrap" data-group="<%= group %>">
    <h2><%= group %></h2>
    <div class="round5-top winner5" data-rank="1x16x8x9x5x12x4x13x6x11x3x14x7x10x2x15">&nbsp;</div>
    <div class="round4-topwrap">
      <div class="round4-top" data-rank="1x16x8x9x5x12x4x13">&nbsp;</div>
      <div class="round3-topwrap">
        <div class="round3-top" data-rank="1x16x8x9">&nbsp;</div>
        <div class="round2-topwrap">
          <div class="round2-top" data-rank="1x16">&nbsp;</div>
          <div class="round1-top" data-team="<%= teams[1].id %>" data-rank="1">1. <%= team_tag(teams[1]) %></div>
          <div class="round1-bottom" data-team="<%= teams[16].id %>" data-rank="16">16. <%= team_tag(teams[16]) %></div>
        </div>
        <div class="round2-bottomwrap">
          <div class="round2-bottom" data-rank="8x9">&nbsp;</div>
